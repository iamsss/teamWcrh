<div id="login" class="modal">
    <div class="modal-content">
        <div class="row">
            <div class="col m8 offset-m2">
              <h2 class="text-center title">Sign In Form</h2>
               <form id="contact-form"  onsubmit="login(); return false;" class="contact-form">
                <div class="row">
                  <div class="col s12">
                    <div class="form-group bmd-form-group">
                      <label class="bmd-label-floating">Username</label>
                      <input id="userName" type="text" required class="form-control validate">
                      <span id="userNameErrorMsg" class="helper-text" data-error="Please enter your username"></span>
                    </div>
                  </div>
                  <div class="col s12">
                    <div class="form-group bmd-form-group">
                      <label class="bmd-label-floating">Password</label>
                      <input id="userPassword" type="password" required class="form-control validate">
                      <span id="passwordErrorMsg" class="helper-text" data-error="Please enter your password"></span>
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-4 ml-auto mr-auto text-center">
                    <input type="submit" class="btn btn-primary btn-raised pink" value="Sign In">
                     
                  </div>
                </div>
              </form>
            </div>
          </div>
    </div>
  </div>
  <script>
    function UserLogin() {
     return {
      UserName : $("#userName").val(),
      Password : $("#userPassword").val()
    }
    }
    function login(){
      var user = UserLogin();
      alert(user.UserName + " - " + user.Password);
    }
  </script>
            
          